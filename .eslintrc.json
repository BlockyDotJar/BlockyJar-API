{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "function-paren-newline": [ "off", "multiline" ],
        "no-unexpected-multiline": [ "off" ]
    }
}
